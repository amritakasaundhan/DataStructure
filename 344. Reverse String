class Solution {
    public void reverseString(char[] s) {
        if(s.length == 0 || s == null){
            return ;
        }
        int start = 0;
        int end = s.length-1;
        while(start < end){
            swap(s, start, end);
            start++;
            end--;
        }
    }
    public void swap(char[] s, int i, int j){
        char temp = s[i];
        s[i] = s[j];
        s[j] = temp;
    }
}

class Solution {
    public void reverseString(char[] s) {
        int start = 0; 
        int end = s.length-1;
        while(start < end){
            char temp = s[start];
            s[start] = s[end];
            s[end] = temp;
            start++;
            end--;
        }
    }
}
