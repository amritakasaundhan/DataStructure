/**
 * Definition for a binary tree node.
 * public class TreeNode {
 *     int val;
 *     TreeNode left;
 *     TreeNode right;
 *     TreeNode(int x) { val = x; }
 * }
 */
class Solution {
    public TreeNode lowestCommonAncestor(TreeNode root, TreeNode p, TreeNode q) {
        if(root == null){
            return null;
        }

        if(root == p){
            return root;
        }
        if(root == q){
            return root;
        }

        TreeNode left_childs = lowestCommonAncestor(root.left, p , q);
        TreeNode right_childs = lowestCommonAncestor(root.right, p , q);
        if(left_childs != null && right_childs != null){
            return root;
        }
        if(left_childs != null){
            return left_childs;
            
        } else if (right_childs != null){
            return right_childs;
        } else{
            return null;
        }
        
    }
}
