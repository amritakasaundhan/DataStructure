class Solution {
    public int maxProfit(int[] prices) {
        int max_profit = 0;
        int minprice = Integer.MAX_VALUE;
        for(int i = 0; i < prices.length;i++){
           if(prices[i] < minprice) {
               minprice = prices[i];
           }else if((prices[i] - minprice) > max_profit ){
               max_profit = prices[i] - minprice;
           }
        }
       return max_profit; 
    }
}



